{
    "presets": [
        ["env", {
            "modules": "commonjs"
        }],
        "stage-2"
    ],
    "plugins": [
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ],
    "env": {
        "development": {
            "plugins": [
                ["module-resolver", {
                    "root": ["./"],
                    "alias": {}
                }]
            ]
        },
        "production": {
            "plugins": [
                ["module-resolver", {
                    "root": ["./"],
                    "alias": {}
                }]
            ]
        }
    }
}